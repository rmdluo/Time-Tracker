{
  "name": "Time Tracker",
  "version": "1.0",
  "manifest_version": 2,
  "background":
  	{
  		"scripts": ["background.js"],
  		"persistant": true
  	},
  "permissions": [
  	"tabs"
  ],
  "browser_action": {
  	"default_popup": "popup.html"
  },
  "icons": {"16": "icon16.png",
  			"48": "icon48.png",
  		   "128": "icon128.png"
  }
}